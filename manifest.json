{
    "name": "Flip Video",
    "action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 3,
    "version": "0.1",
    "description": "Rotates video, 90deg, 180deg, 270deg or standard",
    "permissions": [
      "activeTab",
      "scripting",
      "tabs"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }